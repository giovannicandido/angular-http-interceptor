{
  "name": "angular-http-interceptor",
  "version": "0.2.4",
  "description": "A interceptor module to http calls",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "lint:fix": "tslint ./src/**/*.ts --fix",
    "test": "npm run clean && tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "npm run clean && tsc && karma start karma.conf.js --single-run",
    "compile": "ngc",
    "watch": "ngc --watch",
    "pree2e": "npm run webdriver:update",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "webdriver:update": "webdriver-manager update",
    "copyfiles": "cp -rf package.dist.json dist && cp -rf README.md dist && cp -rf LICENSE dist",
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/angular-http-interceptor.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/angular-http-interceptor.umd.min.js",
    "keepVersion": "export version=`json -f package.json version`;json -I -f package.dist.json -e `echo \"this.version='$version'\"`",
    "build": "npm run transpile && npm run package && npm run minify && npm run copyfiles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atende/angular-http-interceptor.git"
  },
  "keywords": [
    "angular2",
    "interceptor",
    "http"
  ],
  "author": "Giovanni Silva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/atende/angular-http-interceptor/issues"
  },
  "homepage": "https://github.com/atende/angular-http-interceptor#readme",
  "dependencies": {
    "@angular/common": ">=2.0.0 <=3.0.0",
    "@angular/core": ">=2.0.0 <=3.0.0",
    "@angular/http": ">=2.0.0 <=3.0.0",
    "@angular/platform-browser": ">=2.0.0 <=3.0.0",
    "rxjs": ">=5.0.0-beta.12 <= 6.0.0",
    "zone.js": ">=0.6.26 < 2.0.0"
  },
  "devDependencies": {
    "@angular/compiler": "^2.4.2",
    "@angular/compiler-cli": "^2.4.2",
    "@angular/platform-browser-dynamic": ">=2.0.0 <=3.0.0",
    "@types/jasmine": "^2.5.36",
    "@types/node": "^6.0.46",
    "@types/selenium-webdriver": "^2.53.33",
    "angular-in-memory-web-api": "~0.1.13",
    "canonical-path": "0.0.2",
    "concurrently": "^3.1.0",
    "http-server": "^0.9.0",
    "jasmine-core": "~2.5.2",
    "json": "^9.0.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-spec-reporter": "0.0.26",
    "protractor": "4.0.9",
    "reflect-metadata": "^0.1.8",
    "rollup": "^0.41.1",
    "systemjs": "0.19.40",
    "tslint": "^4.0.2",
    "typescript": "^2.1.4",
    "uglify-js": "^2.7.5"
  }
}
